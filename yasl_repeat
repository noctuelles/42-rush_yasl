#!ressources/yasl_linux

{
	1 -
} decrement

{
	0 +
} convert_int

{
	"Usage : ./yasl_repeat <start_num> <value1> [ <value2> [ ...]]\n<start_num> must be a positive integer.\n"
	print
	1 exit
} usage

{
	[#]
	%@ (
		decrement
		%]#[ "0" < ? (usage)
		%]#[ "9" > ? (usage)
	)
} is_an_int

!!
# 2 < ? usage 
0:=:2
2:is_an_int:2

convert_int
= 1 == ? (1 -)
print
